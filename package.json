{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint resources/fsd-ts/src --ext ts,tsx --cache --report-unused-disable-directives --max-warnings 0",
        "lint:perf": "set TIMING=1 && eslint resources/fsd-ts/src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prettier": "pretty-quick --ignore-path .gitignore",
        "prettier:all": "prettier --write . --ignore-path .gitignore",
        "test:watch": "vitest watch",
        "test:run": "set NODE_OPTIONS=--trace-deprecation&& vitest run",
        "test:coverage": "vitest run --coverage",
        "coverage:open": "npx vite preview --outDir ./coverage",
        "test:coverage:open": "yarn run test:coverage && yarn run coverage:open",
        "dep-cruiser:preview": "npx depcruise resources/fsd-ts/src --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph-preview.svg"
    },
    "devDependencies": {
        "@types/node": "^20.4.0",
        "autoprefixer": "^10.4.14",
        "axios": "^1.1.2",
        "framer-motion": "^10.16.4",
        "laravel-vite-plugin": "^0.7.5",
        "postcss": "^8.4.27",
        "tailwindcss": "^3.3.3",
        "@testing-library/user-event": "^14.5.2",
        "@types/react": "^18.2.47",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^1.1.3",
        "dependency-cruiser": "^16.0.0",
        "eslint": "8.56.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "eslint-plugin-testing-library": "^6.2.0",
        "jsdom": "^23.2.0",
        "path": "^0.12.7",
        "prettier": "^3.1.1",
        "pretty-quick": "^3.1.3",
        "typescript": "^5.3.3",
        "unplugin-fonts": "^1.1.1",
        "vite": "^5.0.11",
        "vite-plugin-checker": "^0.6.2",
        "vite-plugin-eslint": "^1.8.1"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-brands-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "laravel-echo": "^1.15.1",
        "path": "^0.12.7",
        "pusher-js": "^8.2.0",
        "sass": "^1.63.6",
        "@tanstack/react-query": "^5.17.9",
        "@tanstack/react-query-devtools": "^5.17.9",
        "classnames": "^2.5.1",
        "dayjs": "^1.11.10",
        "formik": "^2.4.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^4.0.12",
        "react-icons": "^5.0.0",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.21.1",
        "zod": "^3.22.4",
        "zustand": "^4.4.7",
        "vitest": "^1.1.3",
        "@testing-library/react": "^14.1.2"
    }
